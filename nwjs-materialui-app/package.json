{
  "name": "nwjs-materialui-app",
  "version": "1.0.0",
  "description": "A NW.js application using Material-UI, React Router, and node-json-db for managing assets.",
  "main": "http://localhost:3000",
  "node-remote": "http://localhost:3000/*",
  "scripts": {
    "build": "rm -rf build || true; bun build public/index.html --production --splitting  --outdir build",
    "serve": "http-server ./build -p 3000 -P https://localhost:3000? --proxy-options.secure false -S -C ./cert.pem -K ./key.pem"
  },
  "dependencies": {
    "@bitcoin-js/tiny-secp256k1-asmjs": "^2.2.3",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/x-date-pickers": "^8.9.0",
    "@solana/web3.js": "^1.98.2",
    "async-iters-merge-sort": "^1.0.2",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.3.0",
    "bip32": "^5.0.0-rc.0",
    "bitcoinjs-lib": "^6.1.7",
    "buffer": "^6.0.3",
    "create-hmac": "^1.1.7",
    "dayjs": "^1.11.13",
    "exceljs": "^4.4.0",
    "http-server": "^14.1.1",
    "js-file-download": "^0.4.12",
    "node-fetch-cache": "^5.0.2",
    "promise-throttle": "^1.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "rxdb": "^16.13.0",
    "web3": "^4.16.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "typescript": "^4.4.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
